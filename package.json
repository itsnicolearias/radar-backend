{
  "name": "radar-backend",
  "version": "1.0.0",
  "description": "Radar social networking backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrations:run": "npx sequelize db:migrate",
    "migrations:delete": "npx sequelize db:migrate:undo:all",
    "migrations:revert": "npx sequelize db:migrate:undo --name",
    "seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "radar",
    "social",
    "networking",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "zod": "^3.24.1",
    "@hapi/boom": "10.0.1",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.547.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "bcryptjs": "3.0.2",
    "http": "0.0.1-security"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^6.4.17",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "tailwindcss": "4.1.16",
    "postcss": "8.5.6"
  }
}
