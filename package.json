{
  "name": "radar-backend",
  "version": "1.0.0",
  "description": "Radar social networking backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrations:run": "npx sequelize db:migrate",
    "migrations:delete": "npx sequelize db:migrate:undo:all",
    "migrations:revert": "npx sequelize db:migrate:undo --name",
    "seed": "sequelize-cli db:seed:all",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage"
  },
  "keywords": [
    "radar",
    "social",
    "networking",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@hapi/boom": "10.0.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "3.0.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cors": "^2.8.5",
    "crypto": "1.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "http": "0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "0.547.0",
    "morgan": "^1.10.0",
    "next": "16.0.0",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "typescript-eslint": "^8.46.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "postcss": "8.5.6",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.1.9",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
